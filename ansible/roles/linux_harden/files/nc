logger "BREACHDETECTED: attempted to invoke netcat $@"
echo "connect to somewhere:	nc [-options] hostname port[s] [ports] ..."
echo "listen for inbound:	nc -l -p port [-options] [hostname] [port]"
echo "options:"
echo "	-c shell commands	as \`-e\'; use /bin/sh to exec [dangerous!!]"
echo "	-e filename		program to exec after connect [dangerous!!]"
echo "	-b			allow broadcasts"
echo "	-g gateway		source-routing hop point[s], up to 8"
echo "	-G num			source-routing pointer: 4, 8, 12, ..."
echo "	-h			this cruft"
echo "	-i secs			delay interval for lines sent, ports scanned"
echo "	-k			set keepalive option on socket"
echo "	-l			listen mode, for inbound connects"
echo "	-n			numeric-only IP addresses, no DNS"
echo "	-o file			hex dump of traffic"
echo "	-p port			local port number"
echo "	-r			randomize local and remote ports"
echo "	-q secs			quit after EOF on stdin and delay of secs"
echo "	-s addr			local source address"
echo "	-T tos			set Type Of Service"
echo "	-t			answer TELNET negotiation"
echo "	-u			UDP mode"
echo "	-v			verbose [use twice to be more verbose]"
echo "	-w secs			timeout for connects and final net reads"
echo "	-C			Send CRLF as line-ending"
echo "	-z			zero-I/O mode [used for scanning]"
echo "port numbers can be individual or ranges: lo-hi [inclusive];"
echo "hyphens in port names must be backslash escaped (e.g. 'ftp\\-data')."
